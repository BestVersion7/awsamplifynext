generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usertb {
  account_id Int     @id @default(autoincrement())
  first_name String? @db.VarChar(50)
  last_name  String? @db.VarChar(10)
  gender     String? @db.VarChar(1)
}

model account {
  account_id       Int       @id @default(autoincrement())
  first_name       String    @db.VarChar(25)
  last_name        String    @db.VarChar(25)
  account_email    String    @unique @db.VarChar(100)
  account_password String    @db.VarChar(50)
  account_date     DateTime? @default(now()) @db.Timestamp(6)
}

model article {
  article_id        Int       @id @default(autoincrement())
  article_date      DateTime? @default(now()) @db.Timestamp(6)
  article_image     String?   @db.VarChar(100)
  article_title     String    @unique @db.VarChar(100)
  article_image_alt String    @db.VarChar(100)
  article_post      String
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model home {
  homeid    Int     @default(autoincrement())
  firstname String? @db.VarChar(255)

  @@ignore
}

model masteraccount {
  master_account_id       Int       @id @default(autoincrement())
  master_account_email    String    @unique @db.VarChar(100)
  master_account_password String    @db.VarChar(50)
  master_account_date     DateTime? @default(now()) @db.Timestamp(6)
}

model review {
  review_id      Int       @id @default(autoincrement())
  review_date    DateTime? @default(now()) @db.Timestamp(6)
  review_message String?   @db.VarChar(255)
  review_type    String?   @db.VarChar(25)
}
